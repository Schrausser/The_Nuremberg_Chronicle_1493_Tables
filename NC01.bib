@article{mcnally1978,
 ISSN = {00666785},
 URL = {http://www.jstor.org/stable/23563993},
 abstract = {Gregorius monachus, scriptor, exegeta, papa, sanctus, erat imprimis animarum pastor. Per viginti quinque fere annos et libros spiritum delectantes scribebat et Sacram Scripturam in homiliis explanabat, quibus testibus liquet, quantum ipse mundum materialem, senescentem et iam labefactum absolute ac vehementer contempserit. Condicio huius mundi, qui ad extrema venisse et fere in mortis articulo versari videbatur, effecit, ut papa tamquam in speculo magnam orbis calamitatem brevi venturam videret. Homo seu microcosmos, qui magis magisque in dies mori videtur, mundum tamquam macrocosmon mox moriturum praefigurat. De ruina atque miseria universali esse potius gaudendum quam dolendum, spemque in aeterna et non in caduca ponendam autumat Gregorius. Nulla res praeter Deum aestimationem ultimam meretur, inde nulla res propter se est amanda. Istis diversis elementis doctrina huius Sancti de novissimis praecipue coalescit. Ideoque eschatologus iuste et dicitur et est Gregorius.},
