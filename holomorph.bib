@article{BONNEAU2026103761,
title = {{La formule de Cauchy dans les algèbres}},
journal = {{Bulletin des Sciences Mathématiques}},
volume = {206},
pages = {103761},
year = {2026},
issn = {0007-4497},
doi = {10.1016/j.bulsci.2025.103761},
url = {https://www.sciencedirect.com/science/article/pii/S0007449725001873},
author = {{Bonneau, P., & Mazzilli, E.}},
keywords = {Représentations intégrales, Algèbres associatives et non associatives, Equations de Cauchy-Riemann, Equation de Cauchy-Fueter, Integral representations, Associative and non associative algebras, Cauchy-Riemann equations, Cauchy-Fueter equations},
abstract = {Résumé
Dans ce papier, nous établissons la formule de Cauchy pour des fonctions à valeurs dans une algèbre, A, satisfaisant un système d'équations aux dérivées partielles d'ordre 1 à coefficients constants ou variables. Des premiers résultats en ce sens ont été obtenus dans [7] et [8] pour A une super-algèbre et pour des fonctions A-différentiables. Nous généralisons ces résultats et nous faisons le lien avec les équations de Cauchy-Fueter etudiées par différents auteurs, par exemple [4] et [5]. Tout au long du papier, nous obtenons des conditions nécessaires et suffisantes sur l'algèbre A pour avoir des formules de Cauchy, et nous donnons des exemples dans des algèbres naturelles et plus exotiques. Enfin, le théorème de Hartogs est obtenu dans ce contexte ainsi que la construction du premier groupe de cohomologie à support compact.
In this paper, we establish the Cauchy formula for functions f with value in an algebra, A, satisfying PDE system of order one with constant and variable coefficients. The first attempt in this area was obtained in [7] and [8] for superalgebras A and for A-differentiable functions f. We generalize this results and we make the link with the Cauchy-Fueter equations considered by various authors see [4] and [5] for example. We obtain necessary and sufficient conditions on the algebra A to have Cauchy-Formula and all along the paper, we give examples in natural and more exotic algebras. Finally, the Hartogs theorem is proved in this setting and the first cohomology group with compact support is constructed.}
}
