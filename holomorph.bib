@incollection{ULLRICH2005448,
title = {Chapter 34 - Bernhard Riemann, thesis on the theory of functions of a complex variable (1851)},
editor = {{Grattan-Guinness, I., Cooke, R., Leo Corry, L., Crépel, P., & Guicciardini, N.}},
booktitle = {Landmark Writings in Western Mathematics 1640-1940},
publisher = {Elsevier Science},
address = {Amsterdam},
pages = {448--459},
year = {2005},
isbn = {978-0-444-50871-3},
doi = {10.1016/B978-044450871-3/50115-7},
url = {https://www.sciencedirect.com/science/article/pii/B9780444508713501157},
author = {{Ullrich, P.}},
abstract = {Publisher Summary
In his doctoral thesis, Riemann contributed to the foundations of complex function theory with the notion of a function of a complex variable and a discussion of the concept now called a Riemann surface. He characterized functions not by analytic expressions but by their properties, such as the nature and location of their singularities. In application he gave his mapping theorem, which stated that each simply connected (bounded) domain can conformally be mapped onto the unit circle. Complex numbers had entered mathematics in 16th century Italian algebra in connection with the resolution of equations of the 3rd and 4th degree as purely formal expressions. These Abelian integrals and the functions pertaining to them were a highly prestigious area of research during the 19th century. Riemann's approach brought complex function theory to well-known areas of real-variable mathematics. On the other hand, he used equations that implied that a complex (differentiable) function is already determined by its real part.}
}
