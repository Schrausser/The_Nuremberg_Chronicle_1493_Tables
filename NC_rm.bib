@Inbook{Albrecht2000,
author={{Albrecht, E.}},
editor={{Harvey, S.}},
title={The Organization of Vincent of Beauvais' Speculum Maius and of Some Other Latin Encyclopedias},
bookTitle={The Medieval Hebrew Encyclopedias of Science and Philosophy: Proceedings of the Bar-Ilan University Conference},
year={2000},
publisher={Springer Netherlands},
address={Dordrecht},
pages={46--74},
abstract={The contents of medieval Latin encyclopedias have always been considered as a more or less perfect combination of natural history and liberal arts. This idea stems from Roman antiquity when two encyclopedic traditions existed side-by-side. One tradition was represented by Pliny's Historia naturalis (77 c.e.) with its description of the entire cosmos, starting with the heavens, earth, human beings and animals, and concluding with plants and minerals. The other tradition was represented by Varro's Disciplines (ca. 50 B.c.E.) with its concentration on the seven liberal arts: grammar, dialectics, rhetoric, geometry, arithmetic, astronomy and music. Varro also discussed medicine and architecture. The first medieval author to link these two traditions was Isidore of Seville. In his work, the Etymologiae, dating from 636, he discussed, on the one hand, all the liberal arts, as well as medicine and architecture, and, on the other hand, the universe.1},
isbn={978-94-015-9389-2},
doi={10.1007/978-94-015-9389-2_3}
}
